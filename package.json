  "dependencies": {
    "@cucumber/cucumber": "^8.3.0",
    "@qavajs/cli": "^0.0.3",
    "@qavajs/memory": "^1.0.1",
    "@qavajs/po": "^1.0.1",
    "@qavajs/steps-config-loader": "^0.0.1",
    "@qavajs/steps-wdio": "https://github.com/qavajs/steps-wdio.git#ts-config",
    "typescript": "^4.7.3",
